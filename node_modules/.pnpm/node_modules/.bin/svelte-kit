#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nick/workspace/projects/2025/Spotter/node_modules/.pnpm/@sveltejs+kit@2.37.0_@sveltejs+vite-plugin-svelte@6.1.3_svelte@5.38.6_vite@7.1.4__svelte@5.38.6_vite@7.1.4/node_modules/@sveltejs/kit/node_modules:/home/nick/workspace/projects/2025/Spotter/node_modules/.pnpm/@sveltejs+kit@2.37.0_@sveltejs+vite-plugin-svelte@6.1.3_svelte@5.38.6_vite@7.1.4__svelte@5.38.6_vite@7.1.4/node_modules/@sveltejs/node_modules:/home/nick/workspace/projects/2025/Spotter/node_modules/.pnpm/@sveltejs+kit@2.37.0_@sveltejs+vite-plugin-svelte@6.1.3_svelte@5.38.6_vite@7.1.4__svelte@5.38.6_vite@7.1.4/node_modules:/home/nick/workspace/projects/2025/Spotter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nick/workspace/projects/2025/Spotter/node_modules/.pnpm/@sveltejs+kit@2.37.0_@sveltejs+vite-plugin-svelte@6.1.3_svelte@5.38.6_vite@7.1.4__svelte@5.38.6_vite@7.1.4/node_modules/@sveltejs/kit/node_modules:/home/nick/workspace/projects/2025/Spotter/node_modules/.pnpm/@sveltejs+kit@2.37.0_@sveltejs+vite-plugin-svelte@6.1.3_svelte@5.38.6_vite@7.1.4__svelte@5.38.6_vite@7.1.4/node_modules/@sveltejs/node_modules:/home/nick/workspace/projects/2025/Spotter/node_modules/.pnpm/@sveltejs+kit@2.37.0_@sveltejs+vite-plugin-svelte@6.1.3_svelte@5.38.6_vite@7.1.4__svelte@5.38.6_vite@7.1.4/node_modules:/home/nick/workspace/projects/2025/Spotter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
